<?xml version="1.0"?>
<launch>

    <!-- define arguments -->
    <arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>
    <arg name="sim" default="$(optenv SIM !!NO_SIM_SET!!)"/>
    <arg name="teleop" default="$(optenv TELEOP !!NO_TELEOP_SET!!)"/>
    <arg name="python_urx" default="$(optenv PYTHONURX !!NO_PYTHON_URX_SET!!)"/>
    <arg name="ur_ip" default="$(optenv UR_IP !!NO_UR_IP_SET!!)"/>
    <arg name="gripper_ip" default="$(optenv GRIPPER_IP !!NO_GRIPPER_IP_SET!!)"/>
    <arg name="env-script" default="$(optenv ENV_SCRIPT !!NO_ENV_SCRIPT_SET!!)"/>
    <arg name="pc1" default="$(optenv PC1 !!NO_PC1_SET!!)"/>

    <arg name="robot_env" default="$(optenv ROBOT_ENV !!NO_ROBOT_ENV_SET!!)"/>
    <arg name="pkg_robot_config" default="$(optenv PKG_ROBOT_CONFIG !!NO_PKG_ROBOT_CONFIG_SET!!)"/>
    <arg name="paused" default="$(optenv PAUSED !!NO_PAUSED_SET!!)"/>
    <arg name="initial_config" default="$(optenv INITIAL_CONFIG !!NO_INITIAL_CONFIG_SET!!)"/>

    <arg name="pkg_env_config" default="$(optenv PKG_ENV_CONFIG} !!NO_PKG_ENV_CONFIG_SET!!)"/>
    <arg name="pkg_gazebo_worlds" default="$(optenv PKG_GAZEBO_WORLDS !!NO_PKG_GAZEBO_WORLDS_SET!!)"/>
    <arg name="world_name" default="$(optenv WORLD_NAME} !!NO_WORLD_NAME_SET!!)"/>
    <arg name="use_sim_time" default="$(optenv USE_SIM_TIME !!NO_USE_SIM_TIME_SET!!)"/>
    <arg name="gui" default="$(optenv GUI !!NO_GUI_SET!!)"/>
    <arg name="wrist_sr300_sim" default="$(optenv WRIST_SR300_SIM !!NO_WRIST_SR300_SIM_SET!!)"/>


    <!-- startup simulated world -->
    <include file="$(find cob_gazebo_worlds)/launch/world.launch">
        <arg name="robot_env" value="$(arg robot_env)" />
        <arg name="pkg_env_config" value="$(arg pkg_env_config)" />
        <arg name="pkg_gazebo_worlds" value="$(arg pkg_gazebo_worlds)" />
        <arg name="world_name" value="$(arg world_name)"/>
        <arg name="paused" value="$(arg paused)" />
        <arg name="use_sim_time" value="$(arg use_sim_time)" />
        <arg name="gui" value="$(arg gui)" />
    </include>

    <!-- startup simulated robot -->
    <include file="$(find cob_gazebo)/launch/robot.launch" >
        <arg name="robot" value="$(arg robot)" />
        <arg name="sim" value="$(arg sim)" />
        <arg name="teleop" value="$(arg teleop)" />
        <arg name="python_urx" value="$(arg python_urx)" />
        <arg name="ur_ip" value="$(arg ur_ip)" />
        <arg name="gripper_ip" value="$(arg gripper_ip)" />
        <arg name="env-script" value="$(arg env-script)" />
        <arg name="pc1" value="$(arg pc1)" />
        <arg name="robot_env" value="$(arg robot_env)" />
        <arg name="pkg_robot_config" value="$(arg pkg_robot_config)" />
        <arg name="paused" value="$(arg paused)" />
        <arg name="initial_config" value="$(arg initial_config)"/>
        <arg name="wrist_sr300_sim" value="$(arg wrist_sr300_sim)"/>
    </include>

</launch>

